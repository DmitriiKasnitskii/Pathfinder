{
  "parser": "typescript-eslint-parser",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "no-unused-vars": "off", //supress incorrect worn an import rule in typeScript
    "guard-for-in": "off" //supress for in
  },
  "plugins": [
    "typescript"
  ],
  "settings": {
    "import/resolver": ["webpack", "node"]
  }
}
